#ARBITRARY FUNCTIONS
def addition(*n): # * is used to accept a number of various arguments or values
    sum1=0
    for number in n:
        sum1 += number

    print(sum1)
addition(12,3,4,5,6,73,674,25)


def info(**kw): #multiple arbitrary keywords
    for k,v in kw.items():
        print(k,':',v)
info(name= 'ab',
     age = 18,
marks =94 )

#returning values from the user defined functions using return keyword
def subtract(*n):
    sub = 0
    for num in n:
        sub-=num
    return sub
subt = subtract(-20,50)
print(subt)

#PASS keyword is used to avoid the errors
#LAMBDA FUNCTION is used to allocate a function without even defining it
sum = lambda a,b: a*b
print(sum(12,2))
#creating list of sorted unique elements using remover functions
lst = [1,2,3,4,5,6,7,8,93,4,5,6,7,8]
def remove_dup(l):
    rem_dup = set(l)
    b = list(rem_dup)
    b.sort()
    return b
print(lst)
print(remove_dup(lst))

