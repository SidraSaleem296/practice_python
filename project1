class ZeroCubeError(Exception):
  ''' O can't be passed as a cube'''
class Cube():
  def __init__(self,number4):
     number4 = int(number4)
     if number4 != 0:
         self.qub= number4**3
     else:
         raise ZeroCubeError
try:
    number5 = Cube(input('Number_'))
    number5 = Cube(input('Number_'))
except ZeroCubeError:
    print(ZeroCubeError.__doc__)
else :
    print('Quotient:'+str(number5))
    print('cube :'+str((number5).qub))
finally:
    print('execution completed')

