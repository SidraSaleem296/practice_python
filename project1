#print(st3)

# Intersection
print(st3 & st4)
#difference
print(st3-st4) # means elements of st3 which are not present in st4
# symmetric difference
print(st3 ^ st4 ) #it means that the elemnts wchich are not present in either 1 or 2 of the both

