'''def sm_d(*n):
    sm = 0
    d = []
    for num in n:
        sm+=num
        Num = str(num).split('.')
        d.append(len(Num[1]))
    min_d = min(d)
    return round(sm,min_d)
print(sm_d(1.345,2.4566,234.6675,56.77778))
